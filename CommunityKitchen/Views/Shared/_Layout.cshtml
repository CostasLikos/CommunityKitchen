<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Charity sympathy || Responsive HTML 5 Template</title>
    <!-- mobile responsive meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href="~/Assets/css/signuplogin.css" rel="stylesheet" />
    <link href="~/Assets/css/style.css" rel="stylesheet" />
    <link href="~/Assets/css/responsive.css" rel="stylesheet" />
    <link href="~/Assets/css/flaticon.css" rel="stylesheet" />
    <link rel="apple-touch-icon" sizes="180x180" href="~/Assets/images/favicon/apple-touch-icon.png">
    <link href="~/Assets/images/favicon/apple-touch-icon.png" rel="apple-touch-icon" sizes="180x180" />
    <link rel="icon" type="image/png" href="~/Assets/images/favicon/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="~/Assets/images/favicon/favicon-16x16.png" sizes="16x16">

</head>
<body>
    @*Meta Login*@
    <script src="~/Scripts/MetaLogin.js"></script>
    <div class="container23">
        <div class="col-mb-12">
            <!--<header class="top-bar">

            <div class="column left">
            </div>

            <div id="status" class="column center">
            </div>
            <div class="column right">-->
            @*<ul id="log1" class="login-info">
                    <li onclick="loginToggle()" id="log"><a id="logi" href="#"><i class="icon fa fa-arrow-circle-down"></i>login</a></li>
                    <li onclick="signupToggle()" id="log"><a id="logi" href="#"><i class="icon fa fa-user"></i>signup</a></li>
                </ul>*@
            <!--</div>
            </header>-->
